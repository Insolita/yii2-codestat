{
    "name":        "insolita/yii2-codestat" ,
    "description": "Statistic of code usage inspired by Laravel Stats" ,
    "type":        "yii2-extension" ,
    "keywords":    [ "yii2" , "code" , "statistic" ] ,
    "license":     "MIT" ,
    "authors":     [
        {
            "name":  "insolita" ,
            "email": "webmaster100500@ya.ru"
        }
    ] ,
    "require":     {
        "php":           ">=5.6" ,
        "yiisoft/yii2":  "~2.0.13" ,
        "phploc/phploc": "^4.0",
        "league/climate": "^3.2"
    } ,
    "require-dev": {
        "phpunit/phpunit":    "~6.0" ,
        "codeception/verify":  "~1.0.0" ,
        "codeception/specify": "~1.0"
    } ,
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "suggest":     { } ,
    "autoload":    {
        "psr-4": {
            "insolita\\codestat\\": "src/",
            "tests\\":"tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-legacy": "1.0.x-dev"
        }
    }
}
